table.table.table-striped(style="width:100%")
  tbody
    tr(ng-repeat="recent in recents")
      td
        a(ng-href="{{recent.stageNo|listLink}}")
          span STAGE:
          span(ng-bind="recent.stageNo")
        .creator.text-right created by&nbsp;
          span(ng-bind="recent.creator")
        .date.text-right(ng-bind="recent.registDate")

